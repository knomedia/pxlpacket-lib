{
  "name": "pxlpacket-lib",
  "version": "0.0.1",
  "description": "Shared types, zod schemas, and utilities for pxl-packet apps",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rimraf dist",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.cjs"
    }
  },
  "keywords": [
    "pxl-packet"
  ],
  "author": "jsk",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "peerDependencies": {
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "zod": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19"
  },
  "dependencies": {
    "@biomejs/biome": "2.1.4"
  }
}
