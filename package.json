{
  "name": "pxlpacket",
  "version": "0.0.12",
  "description": "Shared types, zod schemas, and utilities for pxl-packet apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knomedia/pxlpacket-lib.git"
  },
  "bugs": {
    "url": "https://github.com/knomedia/pxlpacket-lib/issues"
  },
  "homepage": "https://github.com/knomedia/pxlpacket-lib#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown && tsc -p tsconfig.build.json",
    "dev": "tsdown --watch",
    "clean": "rimraf dist",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm typecheck && pnpm clean && pnpm build",
    "test:unit": "vitest --pool=threads",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "pxl-packet"
  ],
  "author": "jsk",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "peerDependencies": {
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@types/node": "^20.19.19",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^19.0.2",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.0.16"
  },
  "optionalDependencies": {
    "@rolldown/binding-darwin-arm64": "1.0.0-beta.41"
  },
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "package.json": [
        "./package.json"
      ]
    }
  },
  "engines": {
    "node": ">=20.19"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "rolldown",
      "@rolldown/*"
    ]
  }
}
