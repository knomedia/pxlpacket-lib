{
  "name": "pxlpacket",
  "version": "0.0.3",
  "description": "Shared types, zod schemas, and utilities for pxl-packet apps",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rimraf dist",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepare": "pnpm run build",
    "test:unit": "vitest --pool=threads",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    }
  },
  "keywords": [
    "pxl-packet"
  ],
  "author": "jsk",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "peerDependencies": {
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@types/node": "^20.19.19",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^19.0.2",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.0.16"
  },
  "optionalDependencies": {
    "@rolldown/binding-darwin-arm64": "1.0.0-beta.41"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "rolldown",
      "@rolldown/*"
    ]
  }
}
